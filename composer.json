{
    "name": "helstern/nomsky"
    , "description": "cs grammar tools"
    , "minimum-stability": "stable"
    , "license": "proprietary"
    , "authors": [
        {
            "name": "Radu Helstern"
            ,"email": "radu.helstern@gmail.com"
        }
    ]
    , "repositories": [
        {
            "type": "composer",
            "url": "https://packages.zendframework.com/"
        }
    ]

    , "autoload": {
        "psr-4": {
            "Helstern\\Nomsky\\": "../../src/main/php"
        }
    }

