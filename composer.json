        "phpunit/phpunit": "4.2.*"
    }
    , "autoload-dev": {
        "psr-4": {
            "Helstern\\Nomsky\\": ["../../src/test/php"]
        }
    }
    , "config": {
        "vendor-dir" : "../composer"
    }
}
